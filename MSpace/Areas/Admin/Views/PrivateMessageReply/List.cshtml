@model IEnumerable<MSpace.Models.PrivateMessageReply>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>


<table class="table table-condensed table-hover">
    <tr>
        <th>
            MessageId
        </th>
        <th>
            ReplyContent
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MessageId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReplyContent)
        </td>
    </tr>
}

</table>
